{
  "name": "gebruederheitz/wp-block-collapsible",
  "description": "A collapsible / accordion block for gutenberg",
  "type": "library",
  "license": "GPL-3.0-only",
  "autoload": {
    "psr-4": {
      "Gebruederheitz\\GutenbergBlocks\\Collapsible\\": "php/"
    }
  },
  "authors": [
    {
      "name": "Andreas Maros",
      "email": "andreas.maros@gebruederheitz.de"
    }
  ],
  "require": {
    "php": ">=7.3",
    "gebruederheitz/wp-gutenberg-blocks": "~1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.7",
    "szepeviktor/phpstan-wordpress": "^1.1",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "lint": [
      "@phpstan",
      "@phpcs"
    ],
    "phpcs": "phpcs",
    "phpstan": "phpstan analyse -c .phpstan.neon"
  }
}
